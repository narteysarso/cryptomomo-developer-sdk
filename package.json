{
  "name": "cryptomomo-developer-sdk",
  "version": "2.2.7",
  "description": "CryptoMomo Developer SDK - Build web3 applications with Account Abstraction and gasless transactions",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "docs",
    "swagger.yaml"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "docs:swagger": "echo 'Open https://editor.swagger.io and paste swagger.yaml content'",
    "docs:redoc": "echo 'Open https://redocly.github.io/redoc and upload swagger.yaml'",
    "docs:validate": "echo 'Upload swagger.yaml to https://editor.swagger.io to validate'"
  },
  "keywords": [
    "web3",
    "blockchain",
    "ethereum",
    "account-abstraction",
    "gasless",
    "sdk",
    "react",
    "typescript",
    "cryptomomo"
  ],
  "author": "CryptoMomo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narteysarso/cryptomomo-developer-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/narteysarso/cryptomomo-developer-sdk/issues"
  },
  "homepage": "https://github.com/narteysarso/cryptomomo-developer-sdk#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
